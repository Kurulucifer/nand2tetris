/**
 * 1-bit register
 * I have no clue why but my bit got overwritten by And
 */
CHIP Bit {
    IN in, load;
    OUT out;
    
    PARTS:
    Mux(a=outback, b=in, sel=load, out=toDFF);
    DFF(in=toDFF, out=out, out=outback);
}